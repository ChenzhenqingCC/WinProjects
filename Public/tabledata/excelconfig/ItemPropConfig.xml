<?xml version="1.0" encoding="utf-8" ?>
<!-- 
name 字段名称
size 字节长度
system 进制（默认十进制）
type 字段类型
	string 字符串
	int 整数
	float 浮点数
-->
<config>
	<table name="ItemProp" ExcelFile="道具基本表.xlsx" sheet="主属性" sort="编号">
		<fields>
			<field name="编号" type="DWORD" var="dwID"/>
			<field name="名称" type="string" var="strName"/>
			<field name="属性" type="float" var="ftProp"/>
			<field name="图包" type="string" var="strPack"/>
		</fields>
	</table>
</config>